{
  "manifest_version": 3,
  "name": "Website Trigger",
  "description": "This extension will stop you from impulsive buying.",
  "version": "1.0.0",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
    },

  "action": {
    "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ]
}
