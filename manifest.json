{
  "manifest_version": 3,
  "name": "Website Trigger",
  "description": "This extension will stop you from impulsive buying.",
  "version": "1.0.0",

  "action": {
    "default_icon": "logo.html",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ]
}
